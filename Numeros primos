#include <iostream>
using namespace std;

bool esPrimo(int n) {
    if (n <= 1) return false;
    for (int i = 2; i * i <= n; i++) {
        if (n % i == 0) return false;
    }
    return true;
}

int main() {
    int inicio, fin;
    cout << "Ingresa el inicio del rango: ";
    cin >> inicio;
    cout << "Ingresa el fin del rango: ";
    cin >> fin;

    cout << "NÃºmeros primos en el rango: ";
    for (int i = inicio; i <= fin; i++) {
        if (esPrimo(i)) cout << i << " ";
    }
    cout << endl;
    return 0;
}
