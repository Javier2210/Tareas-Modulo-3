#include<iostream>
using namespace std;

int main() {
    int a, b;
    cout << "Ingrese el primer numero: ";
    cin >> a;
       cout << "Ingrese el segundo numero: ";
    cin >> b;
    if (a == b)
        cout << "Ambos numeros son iguales." << endl;
    else
        cout << "Los numeros son diferentes." << endl;
    return 0;
}
