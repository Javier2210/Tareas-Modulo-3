#include <iostream>
using namespace std;

int main() {
    int a, b, c;
    cout << "Ingrese el primer numero: ";
    cin >> a;
    cout << "Ingrese el segundo numero: ";
    cin >> b;
      cout << "Ingrese el tercer numero: ";
    cin >> c;
    cout << "El promedio de los tres numeros es: " << a+b+c / 3 << endl;
    return 0;
}
