#include <iostream>
using namespace std;

int main() {
    int numero, factorial = 1;
    cout << "Ingrese un numero positivo: ";
    cin >> numero;

    if (numero < 0) {
        cout << "El numero debe ser positivo." << endl;
        return 1;
    }

    for (int i = 1; i <= numero; i++) {
        factorial *= i;
    }

    cout << "El factorial de " << numero << " es " << factorial << endl;
    return 0;
}
