#include <iostream>
using namespace std;

int main() {
    int a, b;
    cout << "Ingrese el primer numero: ";
    cin >> a;
    cout << "Ingrese el segundo numero: ";
    cin >> b;
    if (b !=0)
        cout << "La division es: " << a / b << endl;
    else 
        cout << "Error, el divisor no puede ser cero";
    return 0;
}
